<!DOCTYPE html>
<html lang="en-us">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="{{ main.css }}" rel="stylesheet">
  </head>
  <body>
    <div id="app"></div>

    {% if window %}
    <script>
      {% for key, value in window %}
        window.{{ key }} = {{ value | safe }};
      {% endfor %}
    </script>
    {% endif %}

    <script src="{{ main.js }}"></script>
  </body>
</html>
